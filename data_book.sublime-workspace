, in that it processes elements in the training set one at a time.},\n	language = {en},\n	urldate = {2013-06-17},\n	journal = {Wikipedia, the free encyclopedia},\n	month = may,\n	year = {2013},\n	note = {Page Version {ID:} 557301943},\n	file = {Snapshot:/home/mackenza/.mozilla/firefox/w5dcspnj.default/zotero/storage/P3MWEXNI/index.html:text/html}\n},\n\n\n@book{mol_body_2003,\n	address = {Durham, {N.C}},\n	title = {The Body Multiple: Ontology in Medical Practice},\n	isbn = {9780822329022},\n	lccn = {AHB},\n	shorttitle = {The Body Multiple},\n	publisher = {Duke University Press},\n	author = {Mol, Annemarie},\n	year = {2003},\n	keywords = {Atherosclerosis, Ethnographic informants, Medical anthropology, Medicine, Netherlands, Ontology, Philosophy, Social medicine}\n},\n\n\n@book{wilson_affect_2010,\n	title = {Affect and artificial intelligence},\n	publisher = {University of Washington Press},\n	author = {Wilson, Elizabeth A.},\n	year = {2010},\n	file = {[HTML] from jhu.edu:/home/mackenza/.mozilla/firefox/w5dcspnj.default/zotero/storage/GSB9IW7T/after_southern_modernism.html:text/html}\n}",
